<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://ws.wso2.org/dataservice" elementFormDefault="qualified" attributeFormDefault="unqualified">

<xs:element name="ProductLines">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="ProductLine" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="textDescription" type="xs:string"/>
            <xs:element name="htmlDescription" type="xs:string"/>
          </xs:sequence>
          <xs:attribute name="productLine" type="xs:string"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

</xs:schema>
