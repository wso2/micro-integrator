<proxy xmlns="http://ws.apache.org/ns/synapse" name="Soap12FaultCodeSenderTestCaseProxy" startOnLoad="true" statistics="disable" trace="disable" transports="http,https">
    <target>
        <inSequence>
            <sequence key="Soap12FaultCodeSenderTestCaseSeq"/>
        </inSequence>
        <outSequence>
        </outSequence>
        <faultSequence>
            <makefault version="soap12">
                <code xmlns:tns="http://www.w3.org/2003/05/soap-envelope" value="tns:Receiver"/>
                <reason value="Default Fault sequence Executed instead of Soap12FaultCodeSenderTestCaseErrorSeq"/>
            </makefault>
            <log level="full">
                <property name="ERROR_CODE" expression="get-property('ERROR_CODE')"/>
                <property name="ERROR_MESSAGE" expression="get-property('ERROR_MESSAGE')"/>
            </log>
            <property name="RESPONSE" value="true"/>
            <header name="To" action="remove"/>
            <send/>
        </faultSequence>
    </target>
    <description/>
</proxy>